{
  "name": "@ohos/ohpm-repo",
  "version": "5.0.4",
  "description": "this is a private ohpm registry.",
  "ohos": {
    "org": "huawei"
  },
  "bin": {
    "ohpm-repo": "./bin/repo.js"
  },
  "scripts": {
    "preinstall": "export PATH=$PATH:./bin",
    "postinstall": "npm run ohpm:certificates",
    "ohpm:certificates": "mkdir -p /app/ohpm-repo/certificates && cp -r certificates/* /app/ohpm-repo/certificates && ls -l /app/ohpm-repo/certificates",
    "ohpm:version": "ohpm-repo -v",
    "ohpm:install": "ohpm-repo install",
    "ohpm:start": "ohpm-repo start",
    "ohpm:bashrc": "touch /app/.bashrc",
    "ohpm:serve": "export PATH=$PATH:./bin && npm run ohpm:certificates && npm run ohpm:version && npm run ohpm:bashrc && npm run ohpm:install && npm run ohpm:start"
  },
  "keywords": [
    "ohpm",
    "ohpm-repo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.6.7",
    "body-parser": "1.20.2",
    "commander": "11.0.0",
    "compressing": "1.6.2",
    "cookie-parser": "1.4.6",
    "express": "4.18.3",
    "fs-extra": "11.2.0",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.4",
    "jszip": "3.10.1",
    "lodash": "4.17.21",
    "log4js": "6.7.1",
    "mysql2": "3.9.7",
    "node-fetch": "2.6.8",
    "node-forge": "1.3.1",
    "node-schedule": "2.1.0",
    "semver": "7.5.4",
    "ssh2-sftp-client": "9.0.4",
    "tar": "6.1.13",
    "typeorm": "0.3.7",
    "uuid": "9.0.1"
  }
}